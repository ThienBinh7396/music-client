{
    "name": "vuetify",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "test": "npm run lint",
        "start": "node server.js"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "chart.js": "^2.9.3",
        "core-js": "^2.6.5",
        "countup.js": "^2.0.4",
        "express": "^4.17.1",
        "hooper": "^0.3.4",
        "material-design-icons-iconfont": "^5.0.1",
        "path": "^0.12.7",
        "sweetalert2": "^8.17.6",
        "velocity-animate": "^2.0.5",
        "vue": "^2.6.10",
        "vue-chartjs": "^3.5.0",
        "vue-countup-v2": "^4.0.0",
        "vue-quill-editor": "^3.0.6",
        "vue-router": "^3.1.3",
        "vue-the-mask": "^0.11.1",
        "vue-toasted": "^1.1.27",
        "vue2-perfect-scrollbar": "^1.2.1",
        "vue2-scrollbar": "0.0.3",
        "vuetify": "^2.1.3",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.12.0",
        "@vue/cli-plugin-eslint": "^3.12.0",
        "@vue/cli-service": "^3.12.0",
        "babel-eslint": "^10.0.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "jest": "^26.0.1",
        "sass": "^1.17.4",
        "sass-loader": "^7.1.0",
        "vue-cli-plugin-vuetify": "^1.0.1",
        "vue-template-compiler": "^2.6.10",
        "vuetify-loader": "^1.2.2"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off",
            "no-cond-assign": "warn",
            "no-self-assign": "warn",
            "no-empty": "warn",
            "no-undef": "warn",
            "getter-return": "warn",
            "no-unused-vars": "warn",
            "no-fallthrough": "warn",
            "no-useless-call": "warn",
            "no-useless-escape": "warn"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
